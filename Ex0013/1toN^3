using System;

class Program {
    static void Main() {
    Console.Clear();
      Console.WriteLine("Введите число N");
      int n = int.Parse(Console.ReadLine());
      if(n < 0) n = -n;
      for(int i = 1; i <= n; i++)
      {
       Console.WriteLine($" i = {i} --> {i*i*i}");
      }
    
  
    }
}
